{
  "name": "base-project",
  "version": "1.0.0",
  "description": "Base project with backend, frontend, and shared packages",
  "private": true,
  "main": "dist/index.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "npm run dev --workspace=packages/backend",
    "dev:frontend": "npm run start --workspace=packages/frontend",
    "build:all": "npm run build --workspace=packages/backend && npm run build --workspace=packages/frontend",
    "build:frontend": "npm run build --workspace=frontend",
    "build:backend": "npm run build --workspace=backend",
    "install:all": "npm install && npm install --workspace=packages/backend && npm install --workspace=packages/frontend && npm install --workspace=packages/shared",
    "test": "jest"
  },
